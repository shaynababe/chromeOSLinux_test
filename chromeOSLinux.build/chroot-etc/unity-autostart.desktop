[Desktop Entry]
Type=Application
Name=chromeOSLinux autostart script for Unity
Exec=/usr/local/bin/chromeOSLinux-unity-autostart
OnlyShowIn=Unity;
NoDisplay=true
X-GNOME-Autostart-Phase=Initialization
X-GNOME-Autostart-Notify=true
X-GNOME-AutoRestart=true
